                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:medium"/>
                                    <ruleml:Var iri="buvo:drain"/>
                                </ruleml:Atom>
                                <ruleml:Data>ind.Liq.Waste</ruleml:Data>
                            </ruleml:Not>
                        </ruleml:Not>
                        <ruleml:Not>
                            <ruleml:Not>
                                <ruleml:Fun iri="lovo:is"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:medium"/>
                                    <ruleml:Var iri="buvo:drain"/>
                                </ruleml:Atom>
                                <ruleml:Data>chemicalWaste</ruleml:Data>
                            </ruleml:Not>
                        </ruleml:Not>
                        <ruleml:Not>
                            <ruleml:Not>
                                <ruleml:Fun iri="lovo:is"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:medium"/>
                                    <ruleml:Var iri="buvo:drain"/>
                                </ruleml:Atom>
                                <ruleml:Data>toxicWaste</ruleml:Data>
                            </ruleml:Not>
                        </ruleml:Not>
                        <ruleml:Not>
                            <ruleml:Not>
                                <ruleml:Fun iri="lovo:is"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:medium"/>
                                    <ruleml:Var iri="buvo:drain"/>
                                </ruleml:Atom>
                                <ruleml:Data>toxicWaste</ruleml:Data>
                            </ruleml:Not>
                        </ruleml:Not>
                        <ruleml:Obligation>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:has"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:medium"/>
                                    <ruleml:Var iri="buvo:drain"/>
                                </ruleml:Atom>
                                <ruleml:Data>treatment</ruleml:Data>
