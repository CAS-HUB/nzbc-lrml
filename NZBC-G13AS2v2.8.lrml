        <lrml:PrescriptiveStatement key="NZBC-NZBC.G13AS2:1.0.1.0.1">
            <ruleml:Rule key="NZBC-NZBC.G13AS2:1.0.1.0.1">
                <ruleml:if>
                    <ruleml:And>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:is"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:medium"/>
                                <ruleml:Var iri="buvo:drain"/>
                            </ruleml:Atom>
                            <ruleml:Data>foulWater</ruleml:Data>
                        </ruleml:Expr>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:below"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:elevation"/>
                                <ruleml:Var iri="buvo:drain"/>
                            </ruleml:Atom>
                            <ruleml:Data>ground</ruleml:Data>
                        </ruleml:Expr>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:is"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:type"/>
                                <ruleml:Var iri="buvo:drain"/>
                            </ruleml:Atom>
                            <ruleml:Data>gravityFlow</ruleml:Data>
                        </ruleml:Expr>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:lessThanEqual"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:diameter"/>
                                <ruleml:Var iri="buvo:drain"/>
                            </ruleml:Atom>
                            <ruleml:Data>150 mm</ruleml:Data>
                        </ruleml:Expr>
                    </ruleml:And>
                </ruleml:if>
                <ruleml:then>
                    <ruleml:Obligation>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:complyWith"/>
                            <ruleml:Atom>
                                <ruleml:Var iri="buvo:drain"/>
                            </ruleml:Atom>
                            <ruleml:Data>nzbc.g13as2</ruleml:Data>
                        </ruleml:Expr>
                    </ruleml:Obligation>
                </ruleml:then>
            </ruleml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.G13AS2:1.0.2.0.1">
            <ruleml:Rule key="NZBC-NZBC.G13AS2:1.0.2.0.1">
                <ruleml:if>
                    <ruleml:And>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:is"/>
