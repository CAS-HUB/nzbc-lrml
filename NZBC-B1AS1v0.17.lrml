    <lrml:Statements>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.B1AS1:1.1.0.1">
            <lrml:Rule key="NZBC-NZBC.B1AS1:1.1.0.1">
                <lrml:if>
                    <lrml:Or>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:material"/>
                                <lrml:Var iri="buvo:construction"/>
                            </lrml:Atom>
                            <lrml:Or>
                                <lrml:Data>masonry</lrml:Data>
                                <lrml:Data>timber</lrml:Data>
                                <lrml:Data>earth</lrml:Data>
                            </lrml:Or>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:has"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:material"/>
                                <lrml:Var iri="buvo:construction"/>
                            </lrml:Atom>
                            <lrml:Data>stucco</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:for"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:construction"/>
                            </lrml:Atom>
                            <lrml:Data>drain</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:in"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:glazing"/>
                            </lrml:Atom>
                            <lrml:Data>structure</lrml:Data>
                        </lrml:Expr>
                    </lrml:Or>
                </lrml:if>
                <lrml:then>
                    <lrml:Permission>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:complyWith"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:design"/>
                                <lrml:Var iri="buvo:structure"/>
                            </lrml:Atom>
                            <lrml:Data>nzbc-b1as1</lrml:Data>
                        </lrml:Expr>
                    </lrml:Permission>
                </lrml:then>
            </lrml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.B1AS1:1.2.0.1">
            <lrml:Rule key="NZBC-NZBC.B1AS1:1.2.0.1">
                <lrml:if>
                    <lrml:And>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:structure"/>
                            </lrml:Atom>
                            <lrml:Data>chimney</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:size"/>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:Data>small</lrml:Data>
                        </lrml:Expr>
                    </lrml:And>
                </lrml:if>
                <lrml:then>
                    <lrml:Permission>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:complyWith"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:design"/>
                                <lrml:Var iri="buvo:structure"/>
                            </lrml:Atom>
                            <lrml:Data>nzbc-b1as1</lrml:Data>
                        </lrml:Expr>
                    </lrml:Permission>
                </lrml:then>
            </lrml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.B1AS1:2.1.0.1">
            <lrml:Rule key="NZBC-NZBC.B1AS1:2.1.0.1">
                <lrml:if>
                    <lrml:And>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:material"/>
                                <lrml:Var iri="buvo:construction"/>
                            </lrml:Atom>
                            <lrml:Data>masonry</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:x"/>
                            </lrml:Atom>
                            <lrml:Data>_new</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:x"/>
                            </lrml:Atom>
                            <lrml:Data>nzs4229.clause7.8.3</lrml:Data>
                        </lrml:Expr>
                    </lrml:And>
                </lrml:if>
                <lrml:then>
                    <lrml:And>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:is"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:content"/>
                                    <lrml:Var iri="buvo:x"/>
                                </lrml:Atom>
                                <lrml:Data>deleted</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:replacedWith"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:content"/>
                                    <lrml:Var iri="buvo:x"/>
                                </lrml:Atom>
                                <lrml:Data>rule</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:equal"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:key"/>
                                    <lrml:Var iri="buvo:rule"/>
                                </lrml:Atom>
                                <lrml:Data>2.1.1.1</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                    </lrml:And>
                </lrml:then>
            </lrml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.B1AS1:2.1.1.1">
            <lrml:Rule key="NZBC-NZBC.B1AS1:2.1.1.1">
                <lrml:if>
                    <lrml:And>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:material"/>
                                <lrml:Var iri="buvo:slab"/>
                            </lrml:Atom>
                            <lrml:Data>concrete</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:construction"/>
                                <lrml:Var iri="buvo:slab"/>
                            </lrml:Atom>
                            <lrml:Data>slabOnGround</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:has"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:slabOnGround"/>
                            </lrml:Atom>
                            <lrml:Data>reinforcing</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:has"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:slab"/>
                            </lrml:Atom>
                            <lrml:Data>outsideEdge</lrml:Data>
                        </lrml:Expr>
                    </lrml:And>
                </lrml:if>
                <lrml:then>
                    <lrml:And>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:within"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:extension"/>
                                    <lrml:Var iri="buvo:reinforcing"/>
                                </lrml:Atom>
                                <lrml:Data>outsideEdge</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:lessThanEqual"/>
                                <lrml:Atom>
                                    <lrml:Var iri="buvo:extension"/>
                                </lrml:Atom>
                                <lrml:Data>75 mm</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:is"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:type"/>
                                    <lrml:Var iri="buvo:reinforcing"/>
                                </lrml:Atom>
                                <lrml:Data>meshSheet</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:is"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:type"/>
                                    <lrml:Var iri="buvo:meshSheet"/>
                                </lrml:Atom>
                                <lrml:Data>weldedGrade500E</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:greaterThanEqual"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:weight"/>
                                    <lrml:Var iri="buvo:meshSheet"/>
                                </lrml:Atom>
                                <lrml:Data>2.27 kg/m^2</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:equal"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:weight"/>
                                    <lrml:Var iri="buvo:meshSheet"/>
                                </lrml:Atom>
                                <lrml:Data>1.14 kg/m^2 each direction</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:has"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:joint"/>
                                    <lrml:Var iri="buvo:meshSheet"/>
                                </lrml:Atom>
                                <lrml:Data>overlap</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:inBetween"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:measurement"/>
                                    <lrml:Var iri="buvo:overlap"/>
                                </lrml:Atom>
                                <lrml:Data>crossWire</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:location"/>
                                <lrml:Var iri="buvo:crossWire"/>
                            </lrml:Atom>
                            <lrml:Data>outermost</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:of"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:crossWire"/>
                            </lrml:Atom>
                            <lrml:Data>meshSheet</lrml:Data>
                        </lrml:Expr>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:equal"/>
                                <lrml:Atom>
                                    <lrml:Var iri="buvo:measurement"/>
                                </lrml:Atom>
                                <lrml:Data>max(a+50mm, 150mm, b)</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:spacing"/>
                                <lrml:Var iri="buvo:crossWire"/>
                            </lrml:Atom>
                            <lrml:Data>a</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:dimension"/>
                                <lrml:Var iri="buvo:manufacturerRequirement"/>
                            </lrml:Atom>
                            <lrml:Data>b</lrml:Data>
                        </lrml:Expr>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:equal"/>
                                <lrml:Expr>
                                    <lrml:Fun iri="fuvo:max"/>
                                    <lrml:Atom>
                                        <lrml:Rel iri="buvo:dimension"/>
                                        <lrml:Var iri="buvo:slab"/>
                                    </lrml:Atom>
                                </lrml:Expr>
                                <lrml:Data>18 m</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:inBetween"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:dimension"/>
                                <lrml:Var iri="buvo:slab"/>
                            </lrml:Atom>
                            <lrml:Data>freeJoint</lrml:Data>
                        </lrml:Expr>
                    </lrml:And>
                </lrml:then>
            </lrml:Rule>
