    <lrml:Statements>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.B1AS3:1.1.1.0.1">
            <lrml:Rule key="NZBC-NZBC.B1AS3:1.1.1.0.1">
                <lrml:if>
                    <lrml:And>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:construction"/>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:Or>
                                <lrml:Data>brickwork</lrml:Data>
                                <lrml:Data>concrete</lrml:Data>
                                <lrml:Data>precastPumiceConcrete</lrml:Data>
                            </lrml:Or>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:connectTo"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:Data>building</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:construction"/>
                                <lrml:Var iri="buvo:building"/>
                            </lrml:Atom>
                            <lrml:Or>
                                <lrml:Data>timberFrame</lrml:Data>
                                <lrml:Data>masonry</lrml:Data>
                            </lrml:Or>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:complyWith"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:construction"/>
                                <lrml:Var iri="buvo:building"/>
                            </lrml:Atom>
                            <lrml:Or>
                                <lrml:Data>NZS3604</lrml:Data>
                                <lrml:Data>NZS4229</lrml:Data>
                            </lrml:Or>
                        </lrml:Expr>
                    </lrml:And>
                </lrml:if>
                <lrml:then>
                    <lrml:Obligation>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:complyWith"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:Data>nzbc-b1as3</lrml:Data>
                        </lrml:Expr>
                    </lrml:Obligation>
                </lrml:then>
            </lrml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.B1AS3:1.1.2.0.1">
            <lrml:Rule key="NZBC-NZBC.B1AS3:1.1.2.0.1">
                <lrml:if>
                    <lrml:And>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:has"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:Data>foundation</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Expr>
                                <lrml:Fun iri="fuvo:topOf"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:slab"/>
                                    <lrml:Var iri="buvo:foundation"/>
                                </lrml:Atom>
                            </lrml:Expr>
                            <lrml:Data>x</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Expr>
                                <lrml:Fun iri="fuvo:topOf"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:stack"/>
                                    <lrml:Var iri="buvo:chimney"/>
                                </lrml:Atom>
                            </lrml:Expr>
                            <lrml:Data>y</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:inBetween"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:height"/>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:And>
                                <lrml:Data>x</lrml:Data>
                                <lrml:Data>y</lrml:Data>
                            </lrml:And>
                        </lrml:Expr>
                    </lrml:And>
                </lrml:if>
                <lrml:then>
                    <lrml:Prohibition>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:greaterThan"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:height"/>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:Data>9 m</lrml:Data>
                        </lrml:Expr>
                    </lrml:Prohibition>
                </lrml:then>
            </lrml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.B1AS3:1.1.2.0.2">
            <lrml:Rule key="NZBC-NZBC.B1AS3:1.1.2.0.2">
                <lrml:if>
                    <lrml:And>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:is"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:Data>cantilevered</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:at"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:base"/>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:Data>roof</lrml:Data>
                        </lrml:Expr>
                    </lrml:And>
                </lrml:if>
                <lrml:then>
                    <lrml:Prohibition>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:greaterThan"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:height"/>
                                <lrml:Var iri="buvo:chimney"/>
                            </lrml:Atom>
                            <lrml:Data>2.4 m</lrml:Data>
                        </lrml:Expr>
                    </lrml:Prohibition>
                </lrml:then>
            </lrml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.B1AS3:1.1.3.0.1.a.1">
            <lrml:Rule key="NZBC-NZBC.B1AS3:1.1.3.0.1.a.1">
                <lrml:if>
                    <lrml:And>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Expr>
                                <lrml:Fun iri="fuvo:width"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:foundation"/>
                                    <lrml:Var iri="buvo:chimney"/>
                                </lrml:Atom>
                            </lrml:Expr>
                            <lrml:Data>w</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:along"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:measurement"/>
                                <lrml:Var iri="buvo:w"/>
                            </lrml:Atom>
                            <lrml:Data>buildingLine</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Expr>
                                <lrml:Fun iri="fuvo:depth"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:foundation"/>
                                    <lrml:Var iri="buvo:chimney"/>
                                </lrml:Atom>
                            </lrml:Expr>
                            <lrml:Data>d</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:perpendicular"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:measurement"/>
