        <lrml:PrescriptiveStatement key="NZ_nzbc-d1as1#2.6_r1.1.4">
            <ruleml:Rule key="NZ_nzbc-d1as1#2.6_r1.1.4">
                <ruleml:if>
                    <ruleml:Expr>
                        <ruleml:Fun iri="lovo:has"/>
                        <ruleml:Atom>
                            <ruleml:Rel iri="buvo:occupant"/>
                            <ruleml:Var iri="buvo:building"/>
                        </ruleml:Atom>
                        <ruleml:Data xsi:type="xs:string">disabilities</ruleml:Data>
                    </ruleml:Expr>
                </ruleml:if>
                <ruleml:then>
                    <ruleml:And>
                        <lrml:Obligation>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:complyWith"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:access"/>
                                    <ruleml:Var iri="buvo:building"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:token">figure</ruleml:Data>
                            </ruleml:Expr>
                        </lrml:Obligation>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:equal"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:key"/>
                                <ruleml:Var iri="buvo:figure"/>
                            </ruleml:Atom>
                            <ruleml:Data xsi:type="lkm:ruleId">f1.1</ruleml:Data>
                        </ruleml:Expr>
                    </ruleml:And>
                </ruleml:then>
            </ruleml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZ_nzbc-d1as1#2.6_r1.1.5">
            <ruleml:Rule key="NZ_nzbc-d1as1#2.6_r1.1.5">
                <ruleml:if>
                    <ruleml:Expr>
                        <ruleml:Fun iri="lovo:partOf"/>
                        <ruleml:Atom>
                            <ruleml:Var iri="buvo:accessRoute"/>
                        </ruleml:Atom>
                        <ruleml:Data xsi:type="xs:string">buvo:escapeRoute</ruleml:Data>
                    </ruleml:Expr>
                </ruleml:if>
                <ruleml:then>
                    <lrml:Obligation>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:complyWith"/>
                            <ruleml:Atom>
                                <ruleml:Var iri="buvo:accessRoute"/>
                            </ruleml:Atom>
                            <ruleml:Data xsi:type="xs:string">nz_nzbc-c2as1#1.0</ruleml:Data>
                        </ruleml:Expr>
                    </lrml:Obligation>
                </ruleml:then>
            </ruleml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZ_nzbc-d1as1#2.6_r1.2">
            <ruleml:Rule key="NZ_nzbc-d1as1#2.6_r1.2">
                <ruleml:if>
                    <ruleml:Expr>
                        <ruleml:Fun iri="lovo:has"/>
                        <ruleml:Atom>
                            <ruleml:Var iri="buvo:building"/>
                        </ruleml:Atom>
                        <ruleml:Data xsi:type="xs:string">buvo:accessRoute</ruleml:Data>
                    </ruleml:Expr>
                </ruleml:if>
                <ruleml:then>
                    <ruleml:And>
                        <lrml:Obligation>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:complyWith"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:slope"/>
                                    <ruleml:Var iri="buvo:accessRoute"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:token">figure</ruleml:Data>
                            </ruleml:Expr>
                        </lrml:Obligation>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:equal"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:key"/>
                                <ruleml:Var iri="buvo:figure"/>
                            </ruleml:Atom>
                            <ruleml:Data xsi:type="lkm:ruleId">f2.1</ruleml:Data>
                        </ruleml:Expr>
                    </ruleml:And>
                </ruleml:then>
            </ruleml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZ_nzbc-d1as1#2.6_r1.3.1">
            <ruleml:Rule key="NZ_nzbc-d1as1#2.6_r1.3.1">
                <ruleml:if>
                    <ruleml:Or>
                        <ruleml:Not>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:is"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:activity"/>
                                    <ruleml:Var iri="buvo:building"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:string">household units</ruleml:Data>
                            </ruleml:Expr>
                        </ruleml:Not>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:equal"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:riskGroup"/>
                                <ruleml:Var iri="buvo:building"/>
                            </ruleml:Atom>
                            <ruleml:Data xsi:type="xs:string">SH1</ruleml:Data>
                        </ruleml:Expr>
                    </ruleml:Or>
                </ruleml:if>
                <ruleml:then>
                    <lrml:Prohibition>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:has"/>
                            <ruleml:Atom>
                                <ruleml:Var iri="buvo:accessRoute"/>
                            </ruleml:Atom>
                            <ruleml:Data xsi:type="xs:string">step</ruleml:Data>
                        </ruleml:Expr>
                    </lrml:Prohibition>
                </ruleml:then>
            </ruleml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZ_nzbc-d1as1#2.6_r1.3.2">
            <ruleml:Rule key="NZ_nzbc-d1as1#2.6_r1.3.2">
                <ruleml:if>
                    <ruleml:And>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:equal"/>
                            <ruleml:Expr>
                                <ruleml:Fun iri="fuvo:count"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:step"/>
                                    <ruleml:Var iri="buvo:accessRoute"/>
                                </ruleml:Atom>
                            </ruleml:Expr>
                            <ruleml:Data xsi:type="xs:integer">1</ruleml:Data>
                        </ruleml:Expr>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:is"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:purpose"/>
                                <ruleml:Var iri="buvo:step"/>
                            </ruleml:Atom>
                            <ruleml:Data xsi:type="lkm:ruleId">threshold weather stops</ruleml:Data>
                        </ruleml:Expr>
                        <ruleml:Or>
                            <ruleml:Not>
                                <ruleml:Expr>
                                    <ruleml:Fun iri="lovo:is"/>
                                    <ruleml:Atom>
                                        <ruleml:Rel iri="buvo:activity"/>
                                        <ruleml:Var iri="buvo:building"/>
                                    </ruleml:Atom>
                                    <ruleml:Data xsi:type="xs:string">household units</ruleml:Data>
                                </ruleml:Expr>
                            </ruleml:Not>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:equal"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:riskGroup"/>
                                    <ruleml:Var iri="buvo:building"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:string">SH1</ruleml:Data>
                            </ruleml:Expr>
                        </ruleml:Or>
                    </ruleml:And>
                </ruleml:if>
                <ruleml:then>
                    <lrml:Prohibition>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:greaterThan"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:height"/>
                                <ruleml:Var iri="buvo:step"/>
                            </ruleml:Atom>
