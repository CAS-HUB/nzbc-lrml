        <lrml:PrescriptiveStatement key="NZ_D1AS1#2.6_r2.1.2.0.1">
            <ruleml:Rule key="NZ_D1AS1#2.6_r2.1.2.0.1">
                <ruleml:if>
                    <ruleml:And>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:is"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:floor"/>
                                <ruleml:Var iri="buvo:space"/>
                            </ruleml:Atom>
                            <ruleml:Data xsi:type="xs:string">wet</ruleml:Data>
                        </ruleml:Expr>
                        <ruleml:Or>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:is"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:activity"/>
                                    <ruleml:Var iri="buvo:space"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:string">accessRoute</ruleml:Data>
                            </ruleml:Expr>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:is"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:activity"/>
                                    <ruleml:Var iri="buvo:space"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:string">walking</ruleml:Data>
                            </ruleml:Expr>
                        </ruleml:Or>
                        <ruleml:Expr>
                            <ruleml:Fun iri="lovo:is"/>
                            <ruleml:Atom>
                                <ruleml:Rel iri="buvo:floor"/>
                                <ruleml:Var iri="buvo:space"/>
                            </ruleml:Atom>
                            <ruleml:Data xsi:type="lkm:boolean">true</ruleml:Data>
                        </ruleml:Expr>
                    </ruleml:And>
                </ruleml:if>
                <ruleml:then>
                    <ruleml:Or>
                        <lrml:Obligation>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:greaterThanEqual"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:srv"/>
                                    <ruleml:Var iri="buvo:flooring"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:integer">39</ruleml:Data>
                            </ruleml:Expr>
                        </lrml:Obligation>
                        <ruleml:And>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:complyWith"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:surfaceType"/>
                                    <ruleml:Var iri="buvo:floor"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:token">table</ruleml:Data>
                            </ruleml:Expr>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:equal"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:key"/>
                                    <ruleml:Var iri="buvo:table"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:decimal">2.0</ruleml:Data>
                            </ruleml:Expr>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:equal"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:acceptableWetSlipResistance"/>
                                    <ruleml:Var iri="buvo:table"/>
                                </ruleml:Atom>
                                <ruleml:Data xsi:type="xs:string">Yes</ruleml:Data>
                            </ruleml:Expr>
                        </ruleml:And>
                    </ruleml:Or>
                </ruleml:then>
            </ruleml:Rule>
        </lrml:PrescriptiveStatement>
