        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZ_nzbc-d1as1#2.6_r1.1.1.b">
            <ruleml:Rule key="NZ_nzbc-d1as1#2.6_r1.1.1.b">
