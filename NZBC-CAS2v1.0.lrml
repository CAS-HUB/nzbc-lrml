                                    <lrml:Var iri="buvo:p2"/>
                                </lrml:Atom>
                                <lrml:Data>table</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:equal"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:key"/>
                                    <lrml:Var iri="buvo:table"/>
                                </lrml:Atom>
                                <lrml:Data>t1.2</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:equal"/>
                                <lrml:Atom>
                                    <lrml:Var iri="buvo:occupantLoad"/>
                                </lrml:Atom>
                                <lrml:Data>s1+p2</lrml:Data>
