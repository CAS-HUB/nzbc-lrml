        <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS2:1.4.2.0.1">
            <lrml:Rule key="NZBC-NZBC.CAS2:1.4.2.0.1">
                <lrml:if>
                    <lrml:And>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:floorArea"/>
                                <lrml:Var iri="buvo:space"/>
                            </lrml:Atom>
                            <lrml:Data>A</lrml:Data>
                        </lrml:Expr>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:occupantLoadDensity"/>
                            </lrml:Atom>
                            <lrml:Data>d</lrml:Data>
                        </lrml:Expr>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:from"/>
                                <lrml:Atom>
                                    <lrml:Var iri="buvo:d"/>
                                </lrml:Atom>
                                <lrml:Data>table</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:equal"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:key"/>
                                    <lrml:Var iri="buvo:table"/>
                                </lrml:Atom>
                                <lrml:Data>t1.2</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                    </lrml:And>
                </lrml:if>
                <lrml:then>
                    <lrml:Obligation>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:equal"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:occupantLoad"/>
                                <lrml:Var iri="buvo:space"/>
                            </lrml:Atom>
                            <lrml:Data>A/d</lrml:Data>
                        </lrml:Expr>
                    </lrml:Obligation>
                </lrml:then>
            </lrml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS2:1.4.3.0.1">
            <lrml:Rule key="NZBC-NZBC.CAS2:1.4.3.0.1">
                <lrml:if>
                    <lrml:And>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:has"/>
                            <lrml:Atom>
                                <lrml:Var iri="buvo:space"/>
                            </lrml:Atom>
                            <lrml:Data>occupant</lrml:Data>
                        </lrml:Expr>
                        <lrml:Not>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:is"/>
                                <lrml:Atom>
                                    <lrml:Rel iri="buvo:use"/>
                                    <lrml:Var iri="buvo:space"/>
                                </lrml:Atom>
                                <lrml:Data>intermittent</lrml:Data>
                            </lrml:Expr>
                        </lrml:Not>
                    </lrml:And>
                </lrml:if>
                <lrml:then>
                    <lrml:And>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:in"/>
                                <lrml:Expr>
                                    <lrml:Fun iri="fuvo:countDistinct"/>
                                    <lrml:Atom>
                                        <lrml:Rel iri="buvo:occupant"/>
                                        <lrml:Var iri="buvo:space"/>
                                    </lrml:Atom>
                                </lrml:Expr>
                                <lrml:Data>building</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                        <lrml:Expr>
                            <lrml:Fun iri="lovo:define"/>
                            <lrml:Atom>
                                <lrml:Rel iri="buvo:occupantLoad"/>
                                <lrml:Var iri="buvo:space"/>
                            </lrml:Atom>
                            <lrml:Data>p1</lrml:Data>
                        </lrml:Expr>
                        <lrml:Obligation>
                            <lrml:Expr>
                                <lrml:Fun iri="lovo:for"/>
                                <lrml:Atom>
                                    <lrml:Var iri="buvo:p1"/>
                                </lrml:Atom>
                                <lrml:Data>fireSafetyDesign</lrml:Data>
                            </lrml:Expr>
                        </lrml:Obligation>
                    </lrml:And>
                </lrml:then>
            </lrml:Rule>
        </lrml:PrescriptiveStatement>
        <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS2:1.4.4.0.1">
            <lrml:Rule key="NZBC-NZBC.CAS2:1.4.4.0.1">
