                                        <lrml:Var iri="buvo:p"/>
                                    </lrml:Atom>
                                    <lrml:Data>calculated</lrml:Data>
                                </lrml:Expr>
                            </lrml:Not>
                        </lrml:Obligation>
                    </lrml:And>
                </lrml:then>
            </lrml:Rule>
