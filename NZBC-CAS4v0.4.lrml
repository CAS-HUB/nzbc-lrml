  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.c.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.c.1">
      <!-- comment -->
      <ruleml:if>
        <ruleml:Atom>
          <ruleml:Var iri="buvo:escapeRoute" />
          <ruleml:Data iri="TRUE" />
          <Operator iri="lovo:accessibleRoute" />
