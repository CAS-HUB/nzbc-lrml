  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:7.5.6.0.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:7.5.6.0.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <Exp>
            <fun value="count" />
            <ruleml:Atom>
              <ruleml:Var iri="buvo:fireplace" />
              <ruleml:Data iri="1" />
              <Operator iri="lovo:greaterThan" />
            </ruleml:Atom>
          </Exp>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:A" />
            <ruleml:Data iri="fireplace" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:B" />
            <ruleml:Data iri="fireplace" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:A" />
            <ruleml:Data iri="flueSystem" />
            <Operator iri="lovo:has" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:7.5.7.0.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:7.5.7.0.1">
      <!-- comment -->
      <ruleml:if>
        <ruleml:Atom>
          <ruleml:Var iri="buvo:flueJoint" />
        </ruleml:Atom>
      </ruleml:if>
      <ruleml:then>
        <and>
          <obligation>
            <ruleml:Atom>
              <ruleml:Var iri="buvo:flueJoint" />
              <ruleml:Data iri="nonCombustibleMaterial" />
              <Operator iri="lovo:madeOf" />
            </ruleml:Atom>
          </obligation>
          <obligation>
            <ruleml:Atom>
              <ruleml:Var iri="buvo:flueJoint" />
              <ruleml:Data iri="airLeakage" />
              <Operator iri="lovo:sealedAgainst" />
            </ruleml:Atom>
          </obligation>
        </and>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:7.5.8.0.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:7.5.8.0.1">
      <!-- comment -->
      <ruleml:if>
        <ruleml:Atom />
      </ruleml:if>
      <ruleml:then />
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:7.5.9.0.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:7.5.9.0.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:chimney" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:combustibleMaterial" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:clearance" />
            <and>
              <ruleml:Data iri="chimney" />
              <ruleml:Data iri="combustibleMaterial" />
            </and>
            <Operator iri="lovo:between" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:clearance" />
            <ruleml:Data iri="200 mm" />
            <Operator iri="lovo:greaterThanEqual" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:clearance" />
            <or>
              <ruleml:Data iri="flueOpening" />
              <ruleml:Data iri="fireplaceOpening" />
            </or>
            <Operator iri="lovo:at" />
          </ruleml:Atom>
        </and>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:7.5.10.0.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:7.5.10.0.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:hearthEdge" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:combustibleMaterial" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:insulatingMaterial" />
            <and>
              <ruleml:Data iri="hearthEdge" />
              <ruleml:Data iri="combustibleMaterial" />
            </and>
            <Operator iri="lovo:separate" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <ruleml:Atom>
          <ruleml:Rel iri="fuvo:serviceOperatingTemprature" />
          <ruleml:Var iri="buvo:insulatingMaterial" />
          <ruleml:Data iri="150 degreeCelsius" />
          <Operator iri="lovo:greaterThanEqual" />
        </ruleml:Atom>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:7.5.11.0.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:7.5.11.0.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:outerFace" />
            <or>
              <ruleml:Data iri="fireplace" />
              <ruleml:Data iri="chimney" />
              <ruleml:Data iri="flueSystem" />
            </or>
            <Operator iri="lovo:of" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:combustibleMaterial" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <and>
          <obligation>
            <ruleml:Atom>
              <ruleml:Var iri="buvo:ventilatedSpace" />
              <and>
                <ruleml:Data iri="outerFace" />
                <ruleml:Data iri="combustibleMaterial" />
              </and>
              <Operator iri="lovo:between" />
            </ruleml:Atom>
          </obligation>
          <obligation>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:size" />
              <ruleml:Var iri="buvo:ventilatedSpace" />
              <ruleml:Data iri="50 mm" />
              <Operator iri="lovo:greaterThanEqual" />
            </ruleml:Atom>
          </obligation>
        </and>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:7.5.12.0.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:7.5.12.0.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:flueSystem" />
            <ruleml:Data iri="openFire" />
            <Operator iri="lovo:from " />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:section" />
            <ruleml:Var iri="buvo:as/nzs2918" />
            <and>
              <ruleml:Data iri="2" />
              <ruleml:Data iri="4" />
            </and>
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <obligation>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:installation" />
            <ruleml:Data iri="section" />
            <Operator iri="lovo:complyWith" />
          </ruleml:Atom>
        </obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
</lrml:Statements>
