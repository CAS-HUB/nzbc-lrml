          <obligation>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:horizontalWidth" />
              <ruleml:Var iri="buvo:escapeRoute" />
              <ruleml:Data iri="1200 mm" />
              <Operator iri="lovo:greaterThanEqual" />
            </ruleml:Atom>
          </obligation>
          <obligation>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:verticalWidth" />
              <ruleml:Var iri="buvo:escapeRoute" />
              <ruleml:Data iri="1100 mm" />
              <Operator iri="lovo:greaterThanEqual" />
            </ruleml:Atom>
          </obligation>
        </and>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.d.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.d.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <not>
            <ruleml:Atom>
              <ruleml:Var iri="buvo:deadEnd" />
            </ruleml:Atom>
          </not>
          <not>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:key" />
              <ruleml:Var iri="buvo:rule" />
              <ruleml:Data iri="3.13" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </not>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:firecell" />
            <ruleml:Data iri="fireSafetySystem" />
            <Operator iri="lovo:has" />
          </ruleml:Atom>
          <not>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:type" />
              <ruleml:Var iri="buvo:fireSafetySystem" />
              <or>
                <ruleml:Data iri="6" />
                <ruleml:Data iri="7" />
              </or>
              <Operator iri="lovo:include" />
            </ruleml:Atom>
          </not>
          <Exp>
            <fun value="count" />
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:escapeRoute" />
              <ruleml:Var iri="buvo:firecell" />
              <ruleml:Data iri="1" />
              <Operator iri="lovo:greaterThan" />
            </ruleml:Atom>
          </Exp>
          <Exp>
            <fun value="max" />
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:width" />
              <ruleml:Var iri="buvo:escapeRoute" />
              <ruleml:Data iri="x" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </Exp>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:y" />
            <ruleml:Data iri="escapeRoute" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:width" />
            <ruleml:Var iri="buvo:y" />
            <ruleml:Data iri="x" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:unusable" />
            <ruleml:Var iri="buvo:y" />
            <ruleml:Data iri="TRUE" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <obligation>
          <Exp>
            <fun value="average" />
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:horizontalWidth" />
              <ruleml:Var iri="buvo:escapeRoute" />
              <ruleml:Data iri="7 mm/person" />
              <Operator iri="lovo:greaterThanEqual" />
            </ruleml:Atom>
          </Exp>
        </obligation>
        <Exp>
          <fun value="average" />
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:verticalWidth" />
            <ruleml:Var iri="buvo:escapeRoute" />
            <ruleml:Data iri="9 mm/person" />
            <Operator iri="lovo:greaterThanEqual" />
          </ruleml:Atom>
        </Exp>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.e.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.e.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:firecell" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:type" />
            <ruleml:Var iri="buvo:fireSafetySystem" />
            <or>
              <ruleml:Data iri="6" />
              <ruleml:Data iri="7" />
            </or>
            <Operator iri="lovo:include" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <not>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:key" />
            <ruleml:Var iri="buvo:rule" />
            <ruleml:Data iri="3.3.2.0.1.d.1" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
        </not>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.f.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.f.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:escapeRoute" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:escapeDirection" />
            <ruleml:Var iri="buvo:escapeRoute" />
            <ruleml:Data iri="horizontal" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <Exp>
            <fun value="count" />
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:escapeDirection" />
              <ruleml:Var iri="buvo:escapeRoute" />
              <ruleml:Data iri="1" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </Exp>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:anyPoint" />
            <ruleml:Data iri="escapeRoute" />
            <Operator iri="lovo:on" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:anyPoint" />
            <ruleml:Data iri="contributingOccupiedSpace" />
            <Operator iri="lovo:has" />
          </ruleml:Atom>
          <Exp>
            <fun value="SUM" />
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:occupantLoad" />
              <ruleml:Var iri="buvo:contributingOccupiedSpace" />
              <ruleml:Data iri="z" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </Exp>
        </and>
      </ruleml:if>
      <ruleml:then>
        <obligation>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:width" />
            <ruleml:Var iri="buvo:anyPoint" />
            <ruleml:Data iri="7*z" />
            <Operator iri="lovo:greaterThanEqual" />
          </ruleml:Atom>
        </obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.g.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.g.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:x" />
            <ruleml:Data iri="escapeRoute" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:escapeDirection" />
            <ruleml:Var iri="buvo:x" />
            <ruleml:Data iri="horizontal" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <Exp>
            <fun value="count" />
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:escapeDirection" />
              <ruleml:Var iri="buvo:x" />
              <ruleml:Data iri="2" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </Exp>
          <not>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:key" />
              <ruleml:Var iri="buvo:rule" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </not>
        </and>
      </ruleml:if>
      <ruleml:then>
        <ruleml:Atom>
          <ruleml:Var iri="buvo:rule" />
          <ruleml:Data iri="incomplete" />
          <Operator iri="lovo:is" />
        </ruleml:Atom>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.h.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.h.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:firecell" />
            <ruleml:Data iri="intermediateFloor" />
            <Operator iri="lovo:has" />
          </ruleml:Atom>
          <Exp>
            <fun value="SUM" />
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:occupantLoad" />
              <ruleml:Var iri="buvo:contributingOccupiedSpace" />
              <ruleml:Data iri="x" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </Exp>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:component" />
            <ruleml:Var iri="buvo:escapeRoute" />
            <ruleml:Data iri="openPath" />
            <Operator iri="lovo:include" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <and>
          <obligation>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:horizontalTravelWidth" />
              <ruleml:Var iri="buvo:openPath" />
              <ruleml:Data iri="7*x" />
              <Operator iri="lovo:greaterThanEqual" />
            </ruleml:Atom>
          </obligation>
          <obligation>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:verticalTravelWidth" />
              <ruleml:Var iri="buvo:openPath" />
              <ruleml:Data iri="9*x" />
              <Operator iri="lovo:greaterThanEqual" />
            </ruleml:Atom>
          </obligation>
        </and>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.i.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.i.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:type" />
            <ruleml:Var iri="buvo:safePath" />
            <ruleml:Data iri="vertical" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:anyPoint" />
            <ruleml:Data iri="safePath" />
            <Operator iri="lovo:on" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:anyPoint" />
            <ruleml:Data iri="contributingOccupiedSpace" />
            <Operator iri="lovo:has" />
          </ruleml:Atom>
          <Exp>
            <fun value="SUM" />
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:occupantLoad" />
              <ruleml:Var iri="buvo:contributingOccupiedSpace" />
              <ruleml:Data iri="A" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </Exp>
        </and>
      </ruleml:if>
      <ruleml:then>
        <obligation>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:width" />
            <ruleml:Var iri="buvo:anyPoint" />
            <ruleml:Data iri="9*A" />
            <Operator iri="lovo:greaterThanEqual" />
          </ruleml:Atom>
        </obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.j.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.j.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:activity" />
            <ruleml:Var iri="buvo:building" />
            <ruleml:Data iri="marae" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:key" />
            <ruleml:Var iri="buvo:rule" />
            <ruleml:Data iri="4.7.16.0.1.i.1" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:key" />
            <ruleml:Var iri="buvo:rule" />
            <ruleml:Data iri="3.3.2.0.1.b" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:value" />
            <ruleml:Var iri="buvo:rule" />
            <and>
              <ruleml:Data iri="x1" />
              <ruleml:Data iri="x2" />
            </and>
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <and>
          <obligation>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:horizontalWidth" />
              <ruleml:Var iri="buvo:escapeRoute" />
              <ruleml:Data iri="2*x1" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </obligation>
          <obligation>
            <ruleml:Atom>
              <ruleml:Rel iri="fuvo:verticalWidth" />
              <ruleml:Var iri="buvo:escapeRoute" />
              <ruleml:Data iri="2*x2" />
              <Operator iri="lovo:equal" />
            </ruleml:Atom>
          </obligation>
        </and>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.k.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.k.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:a" />
            <ruleml:Data iri="buildingStorey" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:b" />
            <ruleml:Data iri="buildingStorey" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:elevation" />
            <ruleml:Var iri="buvo:a" />
            <ruleml:Data iri="x1" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:elevation" />
            <ruleml:Var iri="buvo:b" />
            <ruleml:Data iri="x2" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:x1" />
            <ruleml:Data iri="x2" />
            <Operator iri="lovo:lessThan" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:escapeRoute" />
            <ruleml:Var iri="buvo:a" />
            <ruleml:Data iri="y1" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:escapeRoute" />
            <ruleml:Var iri="buvo:b" />
            <ruleml:Data iri="y2" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:horizontalWidth" />
            <ruleml:Var iri="buvo:y1" />
            <ruleml:Data iri="A" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:horizontalWidth" />
            <ruleml:Var iri="buvo:y2" />
            <ruleml:Data iri="B" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:finalExit" />
            <ruleml:Var iri="buvo:y1" />
            <ruleml:Data iri="z" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:finalExit" />
            <ruleml:Var iri="buvo:y2" />
            <ruleml:Data iri="z" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <obligation>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:width" />
            <ruleml:Var iri="buvo:z" />
            <ruleml:Data iri="A+B" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
        </obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.l.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.l.1">
      <!-- comment -->
      <ruleml:if>
        <ruleml:Atom>
          <ruleml:Var iri="buvo:escapeRoute" />
          <ruleml:Data iri="ladder" />
          <Operator iri="lovo:include" />
        </ruleml:Atom>
      </ruleml:if>
      <ruleml:then>
        <not>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:key" />
            <ruleml:Var iri="buvo:rule" />
            <ruleml:Data iri="3.3.2.0.1.b.1" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
        </not>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.2.0.1.m.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.2.0.1.m.1">
      <!-- comment -->
      <ruleml:if>
        <or>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:fixedSeating" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:looseSeating" />
          </ruleml:Atom>
        </or>
      </ruleml:if>
      <ruleml:then>
        <not>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:key" />
            <ruleml:Var iri="buvo:rule" />
            <or>
              <ruleml:Data iri="3.3.2.0.1.a.1" />
              <ruleml:Data iri="3.3.2.0.1.b.1" />
              <ruleml:Data iri="3.3.2.0.1.c.1" />
              <ruleml:Data iri="3.3.2.0.1.d.1" />
            </or>
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
        </not>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.3.0.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.3.0.1">
      <!-- comment -->
      <ruleml:if>
        <ruleml:Atom>
          <ruleml:Var iri="buvo:stairway" />
          <ruleml:Data iri="TRUE" />
          <Operator iri="lovo:safeEvacuation" />
        </ruleml:Atom>
      </ruleml:if>
      <ruleml:then>
        <obligation>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:stairway" />
            <ruleml:Data iri="sideHandrail" />
            <Operator iri="lovo:has" />
          </ruleml:Atom>
        </obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.3.0.1.a.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.3.0.1.a.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:escapeRoute" />
            <ruleml:Data iri="stairway" />
            <Operator iri="lovo:include" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:width" />
            <ruleml:Var iri="buvo:stairway" />
            <ruleml:Data iri="1500 mm" />
            <Operator iri="lovo:greaterThan" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <and>
          <obligation>
            <ruleml:Atom>
              <ruleml:Var iri="buvo:stairway" />
              <ruleml:Data iri="sideHandrail" />
              <Operator iri="lovo:has" />
            </ruleml:Atom>
          </obligation>
          <obligation>
            <Exp>
              <fun value="count" />
              <ruleml:Atom>
                <ruleml:Var iri="buvo:sideHandrail" />
                <ruleml:Data iri="2" />
                <Operator iri="lovo:equal" />
              </ruleml:Atom>
            </Exp>
          </obligation>
        </and>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.3.3.0.1.b.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.3.3.0.1.b.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:escapeRoute" />
            <ruleml:Data iri="stairway" />
            <Operator iri="lovo:include" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:width" />
            <ruleml:Var iri="buvo:stairway" />
            <ruleml:Data iri="2000 mm" />
            <Operator iri="lovo:greaterThan" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <and>
          <obligation>
