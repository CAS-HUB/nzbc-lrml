    <ruleml:Rule key="NZBC-NZBC.CAS4:6.1.1.0.1">
      <!-- comment -->
      <ruleml:if>
        <and>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:building" />
            <ruleml:Data iri="property" />
            <Operator iri="lovo:locatedRemotelyFrom" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:property" />
            <ruleml:Data iri="pavement" />
            <Operator iri="lovo:has" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:fireServiceVehicle" />
            <ruleml:Data iri="pavement" />
            <Operator iri="lovo:use" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:ladenWeight" />
            <ruleml:Var iri="buvo:fireServiceVehicle" />
            <ruleml:Data iri="25 tonnes" />
            <Operator iri="lovo:lessThanEqual" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:axleLoad" />
            <ruleml:Var iri="buvo:fireServiceVehicle" />
            <ruleml:Data iri="8 tonnes" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:publicRoad" />
            <ruleml:Data iri="property" />
            <Operator iri="lovo:serve" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="fuvo:loadBearingCapacity" />
            <ruleml:Var iri="buvo:publicRoad" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:x" />
            <ruleml:Data iri="distance" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Var iri="buvo:y" />
            <ruleml:Data iri="distance" />
            <Operator iri="lovo:equal" />
          </ruleml:Atom>
        </and>
      </ruleml:if>
      <ruleml:then>
        <and>
          <obligation>
            <ruleml:Atom>
              <ruleml:Var iri="buvo:pavement" />
              <ruleml:Data iri="min(ladenWeight, loadBearingCapacity)" />
              <Operator iri="lovo:withstand" />
            </ruleml:Atom>
          </obligation>
          <obligation>
            <ruleml:Atom>
              <ruleml:Var iri="buvo:pavement" />
              <ruleml:Data iri="TRUE" />
