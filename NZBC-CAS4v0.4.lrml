  </lrml:PrescriptiveStatement>
  <lrml:PrescriptiveStatement key="NZBC-NZBC.CAS4:3.4.1.0.1.b.1">
    <ruleml:Rule key="NZBC-NZBC.CAS4:3.4.1.0.1.b.1">
