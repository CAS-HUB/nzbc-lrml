        <lrml:PrescriptiveStatement key="NZBC-NZBC.G13AS1:1.0.2.1">
            <ruleml:Rule key="NZBC-NZBC.G13AS1:1.0.2.1">
                <ruleml:if>
                    <ruleml:And>
                        <ruleml:Or>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:equal"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:specialisedTypes"/>
                                    <ruleml:Var iri="buvo:sanitaryFixture"/>
                                </ruleml:Atom>
                                <ruleml:Data>TRUE</ruleml:Data>
                            </ruleml:Expr>
                            <ruleml:Expr>
                                <ruleml:Fun iri="lovo:equal"/>
                                <ruleml:Atom>
                                    <ruleml:Rel iri="buvo:specialisedTypes"/>
                                    <ruleml:Var iri="buvo:sanitaryAppliance"/>
                                </ruleml:Atom>
                                <ruleml:Data>TRUE</ruleml:Data>
